<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".example.grocery_app.ThankYouPage">

  <TextView
      android:id="@+id/textView20"
      android:layout_width="336dp"
      android:layout_height="82dp"
      android:fontFamily="@font/feather_bold_by_typicalbro44_dee263c"
      android:gravity="center"
      android:text="Your Order Has Been Placed!"
      android:textColor="#0E0E25"
      android:textSize="25dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.104" />

  <TextView
      android:id="@+id/textView21"
      android:layout_width="372dp"
      android:layout_height="351dp"
      android:background="@drawable/tybg"
      android:fontFamily="@font/feather_bold_by_typicalbro44_dee263c"
      android:gravity="center"
      android:padding="40dp"
      android:text="Thank you for shopping with us!"
      android:textSize="40dp"
      android:textColor="#000000"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.487"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.465" />

  <TextView
      android:id="@+id/textView22"
      android:layout_width="185dp"
      android:layout_height="61dp"
      android:text="Please contact retailer for payment details"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.8" />

  <Button
      android:id="@+id/button7"
      android:layout_width="282dp"
      android:layout_height="50dp"
      android:text="Return To Home"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.951" />
</androidx.constraintlayout.widget.ConstraintLayout>