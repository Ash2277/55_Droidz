<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
package="com">
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.CALL_PHONE" />
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

<uses-feature
    android:name="android.hardware.telephony"
    android:required="false" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/logo_main"
    android:label="@string/app_name"
    android:networkSecurityConfig="@xml/network_security_config"
    android:fullBackupContent="@xml/full_backup_config"
    android:roundIcon="@mipmap/logo_main_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Grocery_app"
    tools:ignore="GoogleAppIndexingWarning"
    tools:targetApi="n">

    <meta-data
        android:name="com.google.android.geo.API_KEY"
        android:value="@string/google_maps_key" />
    <meta-data
        android:name="com.bumptech.glide.integration.okhttp3.OkHttpGlideModule"
        android:value="GlideModule" />
    <meta-data
        android:name="com.foursite.glide.GlideConfig"
        android:value="GlideModule" />
    <!-- ACTIVITIES -->
    <activity
        android:name="com.pnuema.android.foursite.fullmap.ui.FullMapActivity"
        android:label="@string/title_activity_full_map">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.pnuema.android.foursite.mainscreen.ui.MainActivity" />
    </activity>
    <activity
        android:name="com.pnuema.android.foursite.details.ui.DetailsActivity"
        android:label="@string/title_activity_details"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme.NoActionBar" />
    <activity android:name=".example.grocery_app.BuyingFruits1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingFlour1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingGrains1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingGreens1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingRice1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ContinueWithFruitOrder"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ContinueWithFlourOrder"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ContinueWithVegetableOrder"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ContinueWithRiceOrder"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ContinueWithGreensOrder"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ContinueWithGrainsOrder"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingVegetables1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingFruits2"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.BuyingFruits3"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.SellingGreens"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.SellingFlour"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.SellingRice"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.SellGrains"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.CustomerBuying1"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.CustomerBuying2"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.CustomerBuying3"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.RetailerSignup"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.Homepage"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ThankYouPage"
        android:screenOrientation="portrait"/>

    <!--activity android:name=".example.grocery_app.spam" /-->
    <activity android:name=".example.grocery_app.Login"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ItemsRegistration"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ItemsRegisterationFruits"
        android:screenOrientation="portrait"/>
    <activity android:name=".example.grocery_app.ItemsRegistrationVeggies"
        android:screenOrientation="portrait"/>
    <activity android:name="com.pnuema.android.foursite.mainscreen.ui.MainActivity"
        android:screenOrientation="portrait"/>
    <activity android:name="com.example.grocery_app.Notifications"
        android:screenOrientation="portrait"/>
    <activity
        android:name=".example.grocery_app.LogoScreen"
        android:label="@string/app_name"
        android:noHistory="true"
        android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>
</manifest>

